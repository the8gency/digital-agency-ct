---
layout: layouts/base.njk
eleventyNavigation:
  key: Team
  order: 4
---

<div class="container team-page "> <!-- This doesn't work, FIX later-->
    <h2 class="">Meet Our Team</h2>
    <p class="subTitle">The team that assists you and your company in evolving in a digital world</p>
        <section class="team">
        <div class="row">
            <div class="col">
                <div class="row">
                    {% for person in team %}
                        <div class="col">
                            <div class="card card__team">
                                <img class="card-img-top team__img" src="{{ person.img }}" alt="{{ person.firstName }} {{ person.lastName }}">
                                <div class="card-body team__body">
                                    <h5 class="card-title">{{ person.firstName }} {{ person.lastName }}</h5>
                                    <p class="card-text card-jobfuntion">{{ person.jobFunction }}</p>
                                    <p class="card-text">{{ person.summary }}</p>
                                    <div>
                                        <a href="" class="team__social-link"><i class="fab fa-linkedin-in"></i></a>
                                        <a href="" class="team__social-link"><i class="far fa-envelope"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                        <div class="col-12">
                            <button class="btn btn-tertiary" href="#">Join Our Team</button>
                        </div>
                </div>
            </div>
        </div>
    </section> 
</div>