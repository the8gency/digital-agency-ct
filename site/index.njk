---
<div class="container">
    {% include "components/jumbotron.njk" %}
</div>

<hr class="my-5">

<div class="container">
    <section class="services"> <!-- Sections could probably use extra margins -->
        <div class="row">
            <div class="col">
                <h2 class="display-4">Services</h2>
                <div class="row">
                    {% for service in collections.services %}
                    <div class="col">
                        <div class="card">
                            <h5 class="card-title">{{ service.data.title }}</h5>
                            <p class="card-text">{{service.data.summary}}</p>
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>

    <section class="team">
        <div class="row">
            <div class="col">
                <h2 class="display-4">Team</h2>
                <div class="row">
                    {% for person in team %}
                        <div class="col">
                            <div class="card">
                                <img class="card-img-top" src="{{ person.img }}" alt="{{ person.firstName }} {{ person.lastName }}">
                                <div class="card-body">
                                    <h5 class="card-title">{{ person.firstName }} {{ person.lastName }}</h5>
                                    <p class="card-text">{{ person.jobFunction }}</p>
                                    <a href="#" class="btn btn-primary">Email {{person.firstName}}</a>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>

    <section class="contact">
        <div class="row">
            <div class="col">
                <h2 class="display-4">Contact</h2>
                <p>Contacting people can be fun, and social</p>
                <a href="#" class="btn btn-primary">Contact us</a>
            </div>
        </div>
    </section>

    <section class="clients">
        <div class="row">
            <div class="col">
                <h2 class="display-4">Clients</h2>
                <div class="row">
                    {% for client in clients %}
                        <div class="col">
                            <div class="card">
                                <img class="card-img-top" src="{{ client.logo }}" alt="{{ client.name }}">
                                <div class="card-body">
                                    <h5 class="card-title">{{ client.name }}</h5>
                                    <a href="#" class="btn btn-primary">Read more</a>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>
</div>
