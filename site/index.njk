---
layout: layouts/base.njk
eleventyNavigation:
key: Home
order: 1
title: Home
description: cTÂ² is a Digital Agency. Through hard work and sacrifice we make bugs disappear.
---
<div class="container-fluid jumbotron">
    {% include "components/jumbotron.njk" %}
</div>

<div class="container-fluid about">
    {% include "components/about.njk" %}
</div>


<div class="container">

{% if services | length %}
    {% include "components/services.njk" %}
{% endif %}

{% if projects | length %}
    {% include "components/projects.njk" %}
{% endif %}
  <section class="clients">
        <div class="row">
            <div class="col">
                <div class="row">
                    {% for client in clients %}
                        <div class="col">
                            <div class="card">
                                <img class="card-img-top" src="{{ client.img | url }}" alt="{{ client.name }}" width="150">
                                <div class="card-body">
                                    <h5 class="card-title">{{ client.name }}</h5>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>

    <section class="team">
        <div class="row">
            <div class="col">
                <h2 class="">Meet Our Team</h2>
                <div class="row">
                    {% for person in team %}
                        <div class="col">
                            <div class="card">
                                <img class="card-img-top team__img" src="{{ person.img }}" alt="{{ person.firstName }} {{ person.lastName }}">
                                <div class="card-body team__body">
                                    <h5 class="card-title">{{ person.firstName }} {{ person.lastName }}</h5>
                                    <p class="card-text">{{ person.jobFunction }}</p>
                                    {# <a href="#" class="btn btn-tertiary">Email {{person.firstName}}</a> #}
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>  
</div>
<section class="container-fluid banner">
    <div class="row">
        <div class="col">
            <h3 class="">We care about your project as much as you do.</h3>
            <a href="#" class="btn btn-secondary">Contact Us</a>
        </div>
    </div>
</section>
