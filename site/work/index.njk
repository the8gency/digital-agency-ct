---
layout: layouts/base.njk
tags: 
eleventyNavigation:
  key: Work
  order: 3
---
<div class="container"> <!-- This doesn't work, FIX later-->
  <div class="row">
    {% for work in collections.works %}
      <div class="col">
        <div class="card">
        <a href="{{work.url}}">
          <img class="card-img-top" src="{{work.data.img | url}}" width="360" alt="{{work.data.title}}">
          <div class="card-body">
            <h4 class="display-5">{{work.data.title}}</h4>
            <p class="card-text">{{work.data.subTitle}}</p>
          </div>
          </a>
        </div>
      </div>
    {% endfor %}
  </div>
</div>


