<title>{{ title or site.title }} | {{ subdomain or site.subdomain }} | {{ domain or site.domain }}</title>
<meta name="description" content="{{ description or site.description }}">
<link rel="canonical" href="{{ page.url | url }}" />
<meta property="og:title" content="{{ title or site.title }} | {{ subdomain or site.subdomain }} | {{ domain or site.domain }}"/>
<meta property="og:url" content="{{ page.url | url }}" />
<meta property="og:description" content="{{ description or site.description }}" />
<meta property="og:image" content="{{ '/static/images/logo.jpg' | url }}" />
<meta property="og:type" content="article" />
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@drdynscript">
<meta name="twitter:title" content="{{ title or site.title }} | {{ subdomain or site.subdomain }} | {{ domain or site.domain }}">
<meta name="twitter:description" content="{{ description or site.description }}">
<meta name="twitter:image" content="{{ '/static/images/logo.jpg' | url }}">